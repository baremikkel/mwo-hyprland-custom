{
    "hyprland/workspaces": {
        "on-click": "activate",
        "active-only":false,
        "all-outputs":true,
        "format": "{name}",
        "persistent-workspaces": {
            "*":5
        }
    },
    "clock": {
        "interval":1,
        "format":"{:%H:%M:%S}",
        "on-click":"firefox https://calendar.google.com/calendar/u/0/r?pli=1",
        "tooltip":true,
        "tooltip-format":"{:%A %d. %B %Y (Week %W)}"
    }
}